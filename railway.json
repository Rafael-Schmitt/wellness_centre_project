{
  "$schema": "https://railway.app/railway.schema.json",
  "build": {
    "builder": "NIXPACKS",
    "buildCommand": "./build.sh"
  },
  "deploy": {
    "startCommand": "python manage.py migrate && gunicorn wellness_centre.wsgi",
    "healthcheckPath": "/",
    "healthcheckTimeout": 100,
    "restartPolicyType": "ON_FAILURE"
  }
}